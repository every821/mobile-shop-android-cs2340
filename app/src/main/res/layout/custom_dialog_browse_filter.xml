<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:rsb="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:padding="16dp"
    android:layout_height="match_parent">

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/CUSTOMDIALOGBROWSEFILTER_EDITTEXT_SEARCH"/>

    <TextView
        android:layout_below="@id/CUSTOMDIALOGBROWSEFILTER_EDITTEXT_SEARCH"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/CUSTOMDIALOGBROWSEFILTER_TEXTVIEW_PRICE"
        android:text="Price"
        android:textColor="@color/white"
        android:paddingRight="8dp"
        android:layout_alignParentLeft="true"/>


    <LinearLayout
        android:id="@+id/seekbar_placeholder"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_toRightOf="@id/CUSTOMDIALOGBROWSEFILTER_TEXTVIEW_PRICE"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@id/CUSTOMDIALOGBROWSEFILTER_TEXTVIEW_PRICE"
        android:layout_alignBottom="@id/CUSTOMDIALOGBROWSEFILTER_TEXTVIEW_PRICE" />

</RelativeLayout>